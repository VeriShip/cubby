{
  "name": "vs-cubby",
  "version": "1.0.2",
  "description": "A tool to help make it easier to interact with the cubby hole pattern and HashiCorp Vault",
  "main": "bin/src/index.js",
  "keywords": [
	  "hashicorp", "vault", "cubbyhole"
  ],
  "scripts": {
    "test": "grunt -v test",
    "deploy": "grunt -v deploy",
    "bump": "grunt bump-only -v",
    "dev": "grunt -v test --mochareporter spec",
    "run": "node ./bin/src/index.js"
  },
  "author": "VeriShip",
  "license": "MIT",
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-bump": "^0.7.0",
    "grunt-cli": "^1.1.0",
    "grunt-contrib-coffee": "^1.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-mocha-test": "^0.12.7",
    "grunt-niteo-spawn": "0.0.6",
    "mocha": "^2.4.5",
    "mocha-junit-reporter": "^1.10.0",
    "should": "^8.3.0"
  },
  "dependencies": {
    "aws-sdk": "^2.2.47",
    "check-types": "^6.0.0",
    "commander": "^2.9.0",
    "q": "^1.4.1",
    "string": "^3.3.1"
  },
  "directories": {
    "bin": "./bin/src"
  },
  "bin": {
    "cubby": "./bin/src/index.js"
  }
}
